{
    "plugins": [
        "react",
        "prettier",
        "import",
        "jsx-a11y",
        "react-hooks"
    ],
	"extends": [
		"airbnb",
		"prettier",
		"prettier/react",
		"plugin:react/recommended",
		"eslint:recommended",
		"plugin:prettier/recommended"
	],
    "parser": "babel-eslint",
    "env": {},
    "rules": {
        "no-use-before-define": "off",
        "react/prop-types": "off",
        "comma-dangle": "off",
        "global-require": "off",
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    "jsx"
                ]
            }
        ],
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ],
        "max-len": [
            "error",
            100,
            {
                "ignoreUrls": true
            }
        ],
        "no-param-reassign": "off"
    },
    "globals": {
        "fetch": false
    }
}
